<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#827717"
    android:layout_height="match_parent"
    tools:context=".Event">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.example.loginapp.CurvedBottomNavigationView
            android:id="@+id/bottom_nav"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"

            app:layout_constraintEnd_toStartOf="parent">

        </com.example.loginapp.CurvedBottomNavigationView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:id="@+id/lin_id"
        android:layout_gravity="bottom|center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="40dp"
        android:background="@drawable/oval">

        <com.sdsmdg.harjot.vectormaster.VectorMasterView
            android:id="@+id/fab"
            android:layout_centerInParent="true"
            android:visibility="gone"
            android:layout_margin="12dp"
            android:elevation="6dp"
            app:vector_src="@drawable/ic_account_circle_black_24dp"
            android:layout_width="45dp"
            android:layout_height="45dp"/>

        <com.sdsmdg.harjot.vectormaster.VectorMasterView
            android:id="@+id/fab1"
            android:layout_centerInParent="true"
            android:layout_margin="12dp"
            android:elevation="6dp"
            app:vector_src="@drawable/ic_airplay_black_24dp"
            android:layout_width="45dp"
            android:layout_height="45dp"/>

        <com.sdsmdg.harjot.vectormaster.VectorMasterView
            android:id="@+id/fab3"
            android:layout_centerInParent="true"
            android:visibility="gone"
            android:layout_margin="12dp"
            android:elevation="6dp"
            app:vector_src="@drawable/ic_airport_shuttle_black_24dp"
            android:layout_width="45dp"
            android:layout_height="45dp"/>




    </RelativeLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>

